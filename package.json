{
  "name": "@kronos-integration/interceptor-object-data-processor-row",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/interceptor.mjs",
  "exports": {
    ".": "./src/interceptor.mjs"
  },
  "description": "Splits a line by a sparator into tokens.",
  "keywords": [
    "kronos-interceptor",
    "line",
    "parser",
    "stream"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/interceptor.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/interceptor.mjs"
  },
  "dependencies": {
    "@kronos-integration/interceptor": "^10.1.0",
    "moment": "^2.22.2",
    "z-schema": "5.0.0"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "github:arlac77/documentation",
    "semantic-release": "^17.3.0",
    "underscore": "^1.9.1"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/interceptor-object-data-processor-row.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/interceptor-object-data-processor-row/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/interceptor-object-data-processor-row#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-github-action",
      "arlac77/template-kronos-component"
    ]
  }
}
